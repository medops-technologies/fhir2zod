{
    "name": "fhir2zod",
    "version": "v0.1.0-alpha.1",
    "description": "Convert HL7-FHIR StructureDefinition into Zod Schema.",
    "main": "dist/index.js",
    "bin": {
        "fhir2zod": "./dist/index.js"
    },
    "scripts": {
        "test": "vitest run",
        "test:watch": "vitest",
        "build": "tsc",
        "generate": "ts-node src/index.ts -f examples/hl7.fhir.r4.core@4.0.1/hl7.fhir.r4.core-4.0.1.ndjson -o output",
        "check-lint": "npx @biomejs/biome lint --error-on-warnings",
        "check-format": "npx @biomejs/biome format"
    },
    "author": "Itsuki Sugita",
    "license": "MIT",
    "dependencies": {
        "typescript": "^5.8.3",
        "zod": "^3.24.3"
    },
    "devDependencies": {
        "@biomejs/biome": "1.9.4",
        "@types/node": "^22.14.1",
        "ts-node": "^10.9.2",
        "tsx": "^4.19.3",
        "vitest": "^3.1.2"
    }
}
