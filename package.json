{
  "name": "fhir2zod",
  "version": "0.1.0",
  "description": "Convert HL7-FHIR StructureDefinition into Zod Schema.",
  "main": "dist/index.js",
  "bin": {
    "fhir2zod": "./dist/index.js"
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "build": "tsc",
    "generate": "ts-node src/index.ts -f examples/hl7.fhir.r4.core@4.0.1/hl7.fhir.r4.core-4.0.1.ndjson -o output"
  },
  "author": "Itsuki Sugita",
  "license": "MIT",
  "dependencies": {
    "typescript": "^5.8.3",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "vitest": "^3.1.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3"
  }
}